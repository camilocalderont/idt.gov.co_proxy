{# Template for the view conectate_con_nosotros #}
{% if rows %}
	{% for row in view.result %}
		<a href="{{ row._entity.field_bannerlink.uri }}" class="view-bannersIntermedio" data-aos="zoom-in-up">
			<div class="view-content">
				<img width="1200" height="auto" src="{{ file_url(row._entity.field_banner.entity.uri.value) }}" alt="{{ row._entity.field_banner.alt }}">
			{% endfor %}
		</div>
	{% else %}
		<div class="view-empty">
			{{ empty }}
		</div>
	{% endif %}
</a>
